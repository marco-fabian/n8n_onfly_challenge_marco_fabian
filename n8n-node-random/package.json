{
  "name": "n8n-nodes-random",
  "version": "1.0.0",
  "description": "n8n custom node for generating random numbers using Random.org API",
  "main": "index.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/Random/Random.node.js"
    ]
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests"
  },
  "keywords": [
    "n8n",
    "random",
    "node"
  ],
  "author": "Marco Fabian",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.77",
    "@sentry/node": "^10.14.0",
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.5.2",
    "@types/ssh2": "^1.11.0",
    "axios": "^1.12.2",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "luxon": "^3.7.2",
    "n8n-workflow": "^1.110.0",
    "nock": "^14.0.10",
    "ssh2": "^1.17.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.2"
  }
}
